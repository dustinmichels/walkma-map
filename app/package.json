{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "format": "prettier --write .",
    "minify": "find public/data -type f \\( -name '*.json' -o -name '*.geojson' \\) -exec sh -c 'jq -c . \"$1\" > \"$1.tmp\" && mv \"$1.tmp\" \"$1\"' _ {} \\;",
    "prebuild": "npm run minify"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@tailwindcss/vite": "^4.1.18",
    "lucide": "^0.562.0",
    "lucide-vue-next": "^0.562.0",
    "maplibre-gl": "^5.16.0",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.2.2"
  }
}
